# nix-mox Environment Configuration
# Copy this file to .env and customize with your settings

# Environment type (personal, development, production)
NIXMOX_ENV=personal

# User configuration
NIXMOX_USERNAME=user
NIXMOX_EMAIL=user@example.com
NIXMOX_TIMEZONE=UTC
NIXMOX_HOSTNAME=nixos

# Git configuration
NIXMOX_GIT_USERNAME=user
NIXMOX_GIT_EMAIL=user@example.com

# Security
INITIAL_PASSWORD=changeme
GIT_EMAIL=user@example.com

# Optional: API keys and secrets
# API_KEY=your_api_key_here
# DB_PASSWORD=your_database_password_here
# SSH_PRIVATE_KEY=path_to_your_private_key 